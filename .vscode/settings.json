{
    "json.schemas": [],
    "liveshare.accessibility.soundsEnabled": false,
    "liveshare.accessibility.soundVolume": 10,
    "liveshare.accessibility.voiceVolume": 10,
    "codium.codeCompletion.enable": true,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.typeCheckingMode": "basic",
    "files.autoSave": "onWindowChange",
    "accessibility.signalOptions.volume": 10,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "bracket-pair-colorizer-2.colors": [
        "#fafafa",
        "#9F51B6",
        "#F7C244",
        "#F07850",
        "#9CDD29",
        "#0098FA"
    ],
    "cssModules.camelCase": true,
    "workbench.settings.openDefaultSettings": true,
    "workbench.settings.editor": "json",
    "compile-hero.disable-compile-files-on-did-save-code": false,
    "gitdoc.enabled": false,
    "jest.runMode": {
        "type": "on-save",
        "testFileOnly": true,
        "runAllTestsOnStartup": true
    },
    "jest.outputConfig": {
        "revealOn": "run",
        "revealWithFocus": "test-results",
        "clearOnRun": "none"
    },
    "jest.rootPath": "../client/src",
    "cSpell.enableFiletypes": [
        "!css",
        "!javascript",
        "!javascriptreact",
        "!json",
        "!jsonc"
    ],
    "conventionalCommits.scopes": [
        "test"
    ]
}